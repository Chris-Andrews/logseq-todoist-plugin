{
  "name": "logseq-todoist-plugin",
  "version": "5.2.7",
  "author": "hkgnp",
  "description": "Simple Todoist plugin to pull only active tasks from specified project, i.e. Inbox",
  "logseq": {
    "id": "logseq-todoist-plugin",
    "title": "Todoist",
    "icon": "./icon.png"
  },
  "main": "dist/index.html",
  "targets": {
    "main": false
  },
  "scripts": {
    "build": "rm -R dist && echo 'dist folder removed'; postcss src/tailwind.css -o src/App.css && parcel build src/index.html --public-url ./"
  },
  "license": "MIT",
  "dependencies": {
    "@doist/todoist-api-typescript": "^2.1.2",
    "@logseq/libs": "^0.0.14",
    "logseq-dateutils": "^0.0.26",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^18.17.13",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "buffer": "^6.0.3",
    "cssnano": "^6.0.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "parcel": "^2.9.3",
    "postcss": "^8.4.29",
    "postcss-cli": "^10.1.0",
    "prettier": "3.0.3"
  }
}
