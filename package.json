{
  "name": "logseq-todoist-plugin",
  "version": "5.0.0",
  "author": "hkgnp",
  "description": "Simple Todoist plugin to pull only active tasks from specified project, i.e. Inbox",
  "logseq": {
    "id": "logseq-todoist-plugin",
    "title": "logseq-todoist-plugin",
    "icon": "./icon.png"
  },
  "main": "dist/index.html",
  "targets": {
    "main": false
  },
  "scripts": {
    "build": "rm -R dist && echo 'dist folder removed'; postcss src/tailwind.css -o src/App.css && parcel build --no-source-maps --no-optimize src/index.html --public-url ./"
  },
  "license": "MIT",
  "dependencies": {
    "@doist/todoist-api-typescript": "^2.1.2",
    "@logseq/libs": "^0.0.12",
    "logseq-dateutils": "^0.0.26",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.16",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "buffer": "^5.5.0",
    "cssnano": "^5.1.14",
    "parcel": "^2.8.2",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0"
  }
}
